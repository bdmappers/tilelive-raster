<?xml version="1.0" encoding="utf-8"?>
<Map srs="+proj=merc +a=6378137 +b=6378137 +lat_ts=0.0 +lon_0=0.0 +x_0=0.0 +y_0=0.0 +k=1.0 +units=m +nadgrids=@null +wktext +no_defs +over">
  <Parameters>
    <Parameter name="bounds">{{extent}}</Parameter>
    <Parameter name="center">{{center}}, {{minzoom}}</Parameter>
    <Parameter name="format">png</Parameter>
    <Parameter name="minzoom">{{minzoom}}</Parameter>
    <Parameter name="maxzoom">{{maxzoom}}</Parameter>
    <Parameter name="name"><![CDATA[raster]]></Parameter>
  </Parameters>

  <Style name="raster" filter-mode="first">
    <Rule>
      <RasterSymbolizer opacity="1" />
    </Rule>
  </Style>
  <Layer name="raster" srs="{{projection}}">
    <StyleName>raster</StyleName>
    <Datasource>
      <Parameter name="file"><![CDATA[{{filename}}]]></Parameter>
      <Parameter name="type">gdal</Parameter>
      <Parameter name="nodata">{{nodata}}</Parameter>
    </Datasource>
  </Layer>
</Map>
